@model List<nemtudom.Models.ContentModel>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>title</title>
</head>
<body>
<div>
    <ul>
    @foreach (var item in Model)
    {
        <p>
            @Html.DisplayFor(modelItem => item.content_title)
            <a asp-action="Edit" asp-route-id="@item.content_body">Edit</a>
        </p>
    }
    </ul>
</div>
</body>
</html>